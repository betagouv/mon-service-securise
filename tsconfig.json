{
  "compilerOptions": {
    "target": "es2016",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "allowJs": true
  },
  "include": ["src/**/*", "server.ts", "*.js"]
}
